{
  "name": "noflo-tester",
  "version": "0.0.1",
  "description": "NoFlo component/graph testing wrapper compatible with BDD/TDD/whatever",
  "main": "index.js",
  "scripts": {
    "pretest": "./node_modules/.bin/coffeelint lib/*.coffee spec/*.coffee",
    "test": "./node_modules/.bin/mocha --compilers coffee:coffee-script/register --require coffee-script/register -R spec spec/*.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trustmaster/noflo-tester.git"
  },
  "keywords": [
    "noflo",
    "testing"
  ],
  "author": "Vladimir Sibirov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trustmaster/noflo-tester/issues"
  },
  "homepage": "https://github.com/trustmaster/noflo-tester",
  "dependencies": {
    "bluebird": "^2.3.11",
    "coffee-script": "^1.8.0",
    "noflo": "^0.5.11"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coffeelint": "^1.6.1",
    "mocha": "^2.0.1"
  }
}
